{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/online_mod.js",
      "headers": {
        "cache-control": "public, max-age=60"
      },
      "dest": "public/online_mod.js"
    },
    {
      "src": "/",
      "dest": "public/index.html"
    }
  ]
}